{
    "title": "Shift keys to parentheses",
    "rules": [
        {
            "description": "map left shift to opening parenthesis",
            "manipulators": [
                {
                    "type": "basic",
                    "from": {
                        "key_code": "left_shift",
                        "modifiers": {
                            "optional": [
                                "any"
                            ]
                        }
                    },
                    "to_if_alone": [
                        {
                            "key_code": "9",
                            "modifiers": [
                                "left_shift"
                            ]
                        }
                    ],
                    "to": [
                        {
                            "key_code": "left_shift",
                            "modifiers": []
                        }
                    ]
                }
            ]
        },
        {
            "description": "map right shift to closing parenthesis",
            "manipulators": [
                {
                    "type": "basic",
                    "from": {
                        "key_code": "right_shift",
                        "modifiers": {
                            "optional": [
                                "any"
                            ]
                        }
                    },
                    "to_if_alone": [
                        {
                            "key_code": "0",
                            "modifiers": [
                                "right_shift"
                            ]
                        }
                    ],
                    "to": [
                        {
                            "key_code": "right_shift",
                            "modifiers": []
                        }
                    ]
                }
            ]
        }
    ]
}
